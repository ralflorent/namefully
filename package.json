{
  "name": "namefully",
  "version": "1.0.3",
  "description": "Person name handler in the English alphabet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "webpack --config webpack.dev.js --progress",
    "build:umd": "webpack --config webpack.prod.js --progress",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json --collectCoverage=true",
    "usecases": "npm run build && node ./usecases/index.js",
    "publish": "tsc & npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralflorent/namefully.git"
  },
  "keywords": [
    "fullname",
    "person-name",
    "name-handler",
    "namefull",
    "namefully",
    "firstname",
    "lastname",
    "middlename",
    "prefix",
    "suffix",
    "nickname",
    "surname",
    "givenname"
  ],
  "author": {
    "name": "Ralph Florent",
    "email": "hi@ralflorent.com",
    "url": "https://ralflorent.com"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ralflorent/namefully/issues"
  },
  "homepage": "https://github.com/ralflorent/namefully#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "clean-webpack-plugin": "^3.0.0",
    "jest": "^25.1.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
